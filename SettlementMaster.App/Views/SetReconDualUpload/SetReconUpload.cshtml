<input type="hidden" name="menuId" id="menuId" value="@ViewBag.MenuId" />
@*<div id="right_sidebar">

    </div>*@
@*<div id="content_new">*@
<div class="row" id="upload" style="margin:10px">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <i class="fa fa-upload" style="color:#57ACE7;"></i>  Settlement Reconciliation (Dual-Upload) <a style="color:#E67819" class="btn btn-default btn-xs" href="@Url.Content("~/Uploadtemplate/setrecondualuploadtemplateformat.xlsx")"><i class="fa fa-file-excel-o"></i>  Download Upload Template</a>
            </div>
            <div class="panel-body ">
                <div class="col-lg-12">
                    <div style="background-color:#F4F4F4;margin-bottom:10px;padding:10px">
                        @using (Html.BeginForm("DownloadSetRecon", "SetReconDualUpload", FormMethod.Post, new { id = "formUpload", }))
                        {
                            @Html.AntiForgeryToken()

                            @Html.TextBox("upldfile", "", new { @style = "display:inline", type = "file" })

                            <button id="btnUploada" class="btn btn-default pull-righta"><i class="fa-upload fa"></i> Upload -<b> CLEARED</b></button>
                            <button id="btnUploadb" class="btn btn-default pull-righta" disabled><i class="fa-upload fa"></i> Upload -<b> SWITCH</b></button>
                            <button id="btnValidate" class="btn btn-info pull-righta" disabled style="margin-left:150px"><i class="fa-cog fa" ></i> Process</button>
                                    <button id="btnDownload" class="btn btn-success pull-righta" disabled style="float:right"><i class="fa-file fa"></i> Download</button>


                                <div class="clearfix"></div>
                        }
                    </div>
                    <div id="divGrid">
                        <div class="table-responsive divad2a">
                            <table id="tblUpld" class="table table-striped table-condensed table-bordered">
                                <thead>
                                    <tr>
                                        <th style="width:10%"> S/N</th>
                                        <th style="width:25%">REFERENCE NO.</th>
                                        <th style="width:25%"> CARDTYPE</th>
                                        <th style="width:20%"> TRANSACTION TYPE</th>
                                        <th style="width:20%"> TRANSACTION DATE  </th>
                                        <th style="width:25%"> SETTLEMENT DATE</th>
                                        <th style="width:40%"> MASKEDPAN</th>
                                        <th style="width:20%"> MERCHANTID</th>
                                        <th style="width:20%"> MERCHANT ACCT  </th>
                                        <th style="width:20%"> MERCHANT NAME  </th>
                                        <th style="width:20%"> MERCHANT LOCATION  </th>
                                        <th style="width:30%"> TERMINALID</th>
                                        <th style="width:20%"> TRANSACTION AMOUNT</th>
                                        <th style="width:20%"> AMOUNT CHARGED  </th>
                                        <th style="width:30%"> SET AMOUNT</th>
                                        <th style="width:20%"> MSC RATE</th>
                                        @*<th style="width:20%"> BATCH TYPE</th>*@
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @*</div>*@
</div>
@*<div id="pnlForm" style="display:none" class="panel-form">

    </div>*@
@section Scripts {
    @if (TempData["msg"] != null)
    {
        var msg = @Html.Raw(Json.Encode(TempData["msg"]));
        <script>
        displayDialogNoty('Notification',@msg) ;
        </script>
    }
    @Scripts.Render("~/Scripts/app/setReconDualUpload.js")
}
