                </div>
                <div class="panel-body ">
                    <div class="col-lg-10 col-lg-offset-1">
                        <div style="background-color:#F4F4F4;margin-bottom:10px;padding:10px">
                            @using (Html.BeginForm("", "", FormMethod.Post, new { id = "formUpload", }))
                            {
                                @Html.AntiForgeryToken()
                               
                                @Html.TextBox("upldfile","",new { @style = "display:inline",type="file" })
                                
                                <button id="btnUpload" class="btn btn-default pull-righta"><i class="fa-upload fa"></i> Upload</button>
                                <button id="btnValidate" class="btn btn-info pull-righta" disabled><i class="fa-cog fa"></i> Validate</button>
                                <button id="btnSave" class="btn btn-success pull-righta" disabled><i class="fa-save fa"></i> Save</button>
                                <button id="btnClear" class="btn btn-warning pull-righta"><i class="fa-refresh fa"></i> Reset</button>

                                    <div class="clearfix"></div>
                            }
                        </div>
                        <div id="divGrid">
